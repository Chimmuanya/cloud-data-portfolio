{
  "Version":"2012-10-17",
  "Statement":[
    {
      "Sid":"PackagingBucketWriteRead",
      "Effect":"Allow",
      "Action":["s3:PutObject","s3:GetObject","s3:DeleteObject","s3:ListBucket","s3:GetBucketLocation"],
      "Resource":["arn:aws:s3:::${PACKAGING_BUCKET}","arn:aws:s3:::${PACKAGING_BUCKET}/*"]
    },
    {
      "Sid":"InputBucketReadWriteReports",
      "Effect":"Allow",
      "Action":["s3:GetObject","s3:PutObject","s3:DeleteObject","s3:ListBucket","s3:GetBucketLocation","s3:GetBucketAcl"],
      "Resource":["arn:aws:s3:::508012525512-data-qc-input","arn:aws:s3:::508012525512-data-qc-input/*"]
    }
  ]
}
