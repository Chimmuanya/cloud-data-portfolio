{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PackagingBucketAccess",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:AbortMultipartUpload",
        "s3:ListMultipartUploadParts",
        "s3:GetObject",
        "s3:GetObjectVersion"
      ],
      "Resource": [
        "arn:aws:s3:::cmogbo-sam-pkg-1764511935/*",
        "arn:aws:s3:::508012525512-data-qc-input"
      ]
    },
    {
      "Sid": "PackagingBucketListAndLocation",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::cmogbo-sam-pkg-1764511935",
        "arn:aws:s3:::508012525512-data-qc-input"
      ]
    },
    {
      "Sid": "InputBucketReadAndReports",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::508012525512-data-qc-input",
        "arn:aws:s3:::508012525512-data-qc-input/*"
      ]
    }
  ]
}
