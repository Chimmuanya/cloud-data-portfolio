{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowListAndReadBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::my-bucket"
      ]
    },
    {
      "Sid": "AllowGetObjectsFromProcessedPrefix",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::my-bucket/processed/*"
      ]
    }
  ]
}
